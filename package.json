{
  "name": "arduinogpt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "first-push": "npm run git-init && git add . && git commit -m \"first commit\" && git branch -M main && git remote add origin <YOUR_GITHUB_URL> && git push -u origin main",
    "git-init": "rimraf .git && git init",
    "push": "(git add . && git commit -m \"I'm a bigger pusher than Ice-T homie !!!\" && git push) || exit 0",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "fix-lint-errors": "eslint . --fix",
    "eas-init": "npx eas login && npx eas build:configure",
    "eas-debug-build-android": "npx eas build -p android --profile development",
    "adb-connect": "adb tcpip 57391 && adb connect <ADDR_IP_PHONE>:57391",
    "adb-disconnect": "(adb disconnect) || exit 0",
    "start-debug-android": "(npm run adb-connect && npm run android) || exit 0",
    "start-debug-ios": "(npm run adb-connect && npm run ios) || exit 0",
    "clean-app": "cd android && gradlew clean && cd ..",
    "del-sha-keys": "keytool -delete -v -alias androiddebugkey -keystore android/app/debug.keystore -storepass android",
    "gen-sha-keys": "keytool -genkey -v -keystore android/app/debug.keystore -alias androiddebugkey -storepass android -keypass android -keyalg RSA -validity 14000",
    "get-sha-keys": "keytool -list -v -keystore android/app/debug.keystore -alias androiddebugkey -storepass android"
  },
